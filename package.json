{
  "name": "@carforyou/phraseapp",
  "version": "2.0.0",
  "description": "Phraseapp workflow",
  "scripts": {
    "test": "sh test.sh",
    "build": "pack build",
    "lint": "eslint --ext ts,js,tsx,jsx,json .",
    "format": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/autoricardo/carforyou-phraseapp-pkg.git"
  },
  "author": "CAR FOR YOU",
  "homepage": "https://github.com/autoricardo/carforyou-phraseapp-pkg#readme",
  "devDependencies": {
    "@carforyou/eslint-config": "1.1.0-initial-rules-e5696a2396ab8be5b23db3d5a9dac62f7ba98e51.2",
    "@pika/pack": "^0.3.7",
    "@pika/plugin-build-node": "^0.3.16",
    "@pika/plugin-copy-assets": "^0.3.16",
    "@pika/plugin-simple-bin": "^0.3.16",
    "@pika/plugin-standard-pkg": "^0.3.16",
    "semantic-release": "^17.0.3",
    "typescript": "^3.7.5"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg"
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-copy-assets",
        {
          "files": [
            "bin/"
          ]
        }
      ],
      [
        "@pika/plugin-simple-bin",
        {
          "bin": "cfy-phraseapp"
        }
      ]
    ]
  },
  "dependencies": {
    "glob": "^7.1.4",
    "js-yaml": "^3.13.1",
    "just-diff": "^2.2.0"
  }
}
